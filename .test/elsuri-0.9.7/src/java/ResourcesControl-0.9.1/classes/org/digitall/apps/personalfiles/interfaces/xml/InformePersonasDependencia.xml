            <rectangle name="ta" x="470" y="52"  width="200" height="10" draw="false" fill="false" color="#0000FF"/>
            <string-field        x="470" y="52"  width="200" height="10" alignment="left" dynamic="false" fontsize="08" fsbold="false" fieldname="cuit"/>
            <rectangle name="ta" x="380" y="55"  width="60"  height="10" draw="false" fill="false" color="#0000FF"/>
            <label     name="zo" x="380" y="65"  width="60"  height="10" fontsize="08" dynamic="false" alignment="left" vertical-aligment="middle" fsbold="false">Fecha Impr.:</label>
            <rectangle name="ta" x="470" y="65"  width="200" height="10" draw="false" fill="false" color="#0000FF"/>
            <string-field        x="470" y="65"  width="200" height="10" fontsize="08" dynamic="true"  alignment="left" fieldname="actualdate"/>
            
            <rectangle  name="ta" x="0"   y="90"  width="600"  height="12" draw="false" fill="false" color="#0000FF"/>
            <rectangle  name="ta" x="0"   y="90" width="600"  height="12" draw="false" fill="false" color="#0000FF"/>
            <label      name="zo" x="0"   y="90" width="600"  height="12" fontsize="10" dynamic="false" alignment="center" vertical-aligment="middle" fsbold="true">INFORME PERSONAL - DEPENDENCIAS</label>
            <label      name="zo" x="225"   y="110" width="50"  height="12" fontsize="10" dynamic="false" alignment="right" vertical-aligment="middle" fsbold="true">AÑO: </label>
            <string-field         x="275"   y="110"  width="50" height="12" fontsize="10" dynamic="true"  alignment="left" fsbold="true" fieldname="anio"/>
            
            
            
            <!--<label     name="zo" x="0"   y="85"  width="230"  height="10" fontsize="10" dynamic="false" alignment="left" vertical-aligment="middle" fsbold="false">Recursos por Dependencia</label>-->
            
           
            
        </pageheader>
        
        <groups>
                <group name="Header-Detail">
                        <fields>
			       <field>table_0.iddep</field>
                        </fields>
                        
                        <groupheader layout-cachable="false" pagebreak-before-print="false">
                                <label         x="0" y="0" width="70" dynamic="true" alignment="left" fsbold="true">Dependencia:</label>
				<string-field  x="70" y="0" width="300" height="12" alignment="left" dynamic="false"  fieldname="table_0.dependencia"/>
				
				<rectangle name="tangle"  x="0"     y="14" width="380"  height="16"  draw="true" fill="false" color="#000000"/>
				<rectangle name="tangle"  x="380"   y="14" width="100"  height="16"  draw="true" fill="false" color="#000000"/>
				<rectangle name="tangle"  x="480"   y="14" width="60"  height="16"  draw="true" fill="false" color="#000000"/>
				       
				<label name="resource"     x="0"   y="14" width="380" height="16" fontsize="08" dynamic="true" alignment="center" vertical-alignment="middle">Empleado</label>
				<label name="marca"        x="380" y="14" width="100" height="16" fontsize="08" dynamic="true" alignment="center" vertical-alignment="middle">Fecha de Asignación</label>
				<label name="exixtencia"   x="480" y="14" width="60"  height="16" fontsize="08" dynamic="true" alignment="center" vertical-alignment="middle">Fecha de Inicio</label>
                        </groupheader>
                        
                        <groupfooter height="16" fontname="Monospaced" fontstyle="bold" fontsize="08">
                            <line x1="0" y1="0" x2="540" y2="0" weight="0.5" color="#000000"/>
                        </groupfooter>
                        
                </group>
        </groups>
                
        <items layout-cachable="false" >
            <band name="table_0" fontsize="08">
                    
                    <line x1="0"   y1="0" x2="0"   y2="100%" weight="0.25"/>
                    <line x1="380" y1="0" x2="380" y2="100%" weight="0.25"/>
                    <line x1="480" y1="0" x2="480" y2="100%" weight="0.25"/>
                    <line x1="540" y1="0" x2="540" y2="100%" weight="0.25"/>


                    <string-field name="Empleado"           x="3"    y="2"  width="377" height="10" alignment="left" vertical-alignment="middle" dynamic="false"   fieldname="table_0.empleado"/>
                    <date-field name="Fecha de Asignacion"  x="383"  y="2"  width="97"  height="10" alignment="center" vertical-alignment="middle" dynamic="false" format="dd/MM/yyyy" fieldname="table_0.fechaasignacion"/>
                    <date-field name="Fecha de Inicio"      x="480"  y="2"  width="55"  height="10" alignment="center" vertical-alignment="middle" dynamic="false" format="dd/MM/yyyy" fieldname="table_0.fechainicio"/>
                    
            </band>
            
        </items>

        <reportfooter>
            <line                            x1="450"  y1="540"   x2="550"     y2="540"     weight="0.5"/>
            <label name="Patrimonio"         x="450"   y="550"    width="100"  height="12" fontsize="08" dynamic="true" alignment="center" vertical-alignment="middle">Personal</label>
        </reportfooter>

        <functions>

                <function class="org.jfree.report.function.HideElementByNameFunction" name="hideHeader">
                        <properties>
                                <property name="element">table_0</property>
                                <property name="field">TablePrefix</property>
                        </properties>
                </function>
                
                <function class="org.jfree.report.function.HideElementByNameFunction" name="hideDetail">
                        <properties>
                                <property name="element">table_0</property>
                                <property name="field">TablePrefix</property>
                        </properties>
                </function>

                <function class="org.jfree.report.function.PageOfPagesFunction" name="pageXofY">
                <properties>
                        <property name="format">Página {0} de {1}</property>
                </properties>
            </function>
            
        </functions>
        
        <pagefooter height="24">
                <rectangle name="PF0" x="0" y="0" width="100%" height="100%" color="#FFFFFF" weight="0" draw="false" fill="true"/>
                <line x1="0" y1="0" x2="100%" y2="0" weight="0.5"/>
                <string-field fieldname="location" x="0" y="4" width="100%" height="100%" alignment="left" vertical-alignment="middle"/>
                <string-field fieldname="pageXofY" x="0" y="4" width="100%" height="100%" alignment="right" vertical-alignment="middle"/>
        </pagefooter>
</report>





