<report xmlns="http://jfreereport.sourceforge.net/namespaces/reports/legacy/simple" name="Inventario General Valorado" pageformat="A4"
       topmargin="25" leftmargin="25" bottommargin="10" rightmargin="10">
	
        <reportheader fontname="SanSerif" fontstyle="plain" fontsize="08">
	</reportheader>
	
	 <pageheader onfirstpage="true">

	   <line x1="0" y1="80"  x2="550" y2="80"  weight=".75" color="#AAAAAA"/>
            <line x1="370" y1="0" x2="370" y2="80"  weight=".75" color="#AAAAAA"/>
            
            <image-field name="logo" x="0" y="5" width="52" height="30"  scale="true" fieldname="logo"/>
            <string-field        x="70"  y="00" width="300" height="28" dynamic="false" alignment="center" vertical-aligment="middle" fontsize="14" fsbold="true" fieldname="orgtitle"/>
            <rectangle name="ta" x="70"  y="30" width="300" height="10" draw="false" fill="false" color="#0000FF"/>
            <string-field        x="70"  y="30" width="300" height="10" dynamic="false" alignment="center" fontsize="08" fsbold="false" vertical-aligment="middle" fieldname="locationdata"/>
            <rectangle name="ta" x="0"   y="52" width="230" height="10" draw="false" fill="false" color="#0000FF"/>
	    <string-field        x="0"   y="52" width="230" height="10" dynamic="false" alignment="left" fontsize="08" fieldname="address"/>
            
            <rectangle name="ta" x="240" y="52" width="45"  height="10" draw="false" fill="false" color="#0000FF"/>
            <label     name="ti" x="240" y="52" width="45"  height="10" dynamic="false" alignment="left" fontsize="08" fsbold="false">Teléfonos:</label>
            <rectangle name="ta" x="290" y="52" width="75" height="10" draw="false" fill="false" color="#0000FF"/>
	    <string-field        x="290" y="52" width="75" height="10" dynamic="false" alignment="left" fontsize="08" fieldname="phonenumber1"/>
            <rectangle name="ta" x="290" y="65" width="75" height="10" draw="false" fill="false" color="#0000FF"/>
	    <string-field        x="290" y="65" width="75" height="10" dynamic="false" alignment="left" fontsize="08" fieldname="phonenumber2"/>
            <rectangle name="ta" x="0"   y="65" width="190" height="10" draw="false" fill="false" color="#0000FF"/>
	    <string-field        x="0"   y="65" width="190" height="10" dynamic="false" alignment="left" fontsize="08" fieldname="webaddress"/>

            <label     name="zo" x="380" y="00"  width="200" height="10" fontsize="08" dynamic="false" alignment="center" vertical-aligment="middle" fsbold="true">INVENTARIO GENERAL VALORADO</label>
            
            <rectangle name="ta" x="380" y="30"  width="240" height="12" draw="false" fill="false" color="#0000FF"/>
            <rectangle name="ta" x="380" y="52"  width="60"  height="10" draw="false" fill="false" color="#0000FF"/>
            <label     name="zo" x="380" y="52"  width="60"  height="10" fontsize="08" dynamic="false" alignment="left" vertical-aligment="middle" fsbold="false">C.U.I.T.:</label>
            <rectangle name="ta" x="470" y="52"  width="200" height="10" draw="false" fill="false" color="#0000FF"/>
	    <string-field        x="470" y="52"  width="200" height="10" alignment="left" dynamic="false" fontsize="08" fsbold="false" fieldname="cuit"/>
            <rectangle name="ta" x="380" y="55"  width="60"  height="10" draw="false" fill="false" color="#0000FF"/>
            <label     name="zo" x="380" y="65"  width="60"  height="10" fontsize="08" dynamic="false" alignment="left" vertical-aligment="middle" fsbold="false">Fecha Impr.:</label>
            <rectangle name="ta" x="470" y="65"  width="200" height="10" draw="false" fill="false" color="#0000FF"/>
            <string-field        x="470" y="65"  width="200" height="10" fontsize="08" dynamic="true"  alignment="left" fieldname="actualdate"/>
	    
	    <rectangle  name="ta" x="0"   y="90"  width="600"  height="12" draw="false" fill="false" color="#0000FF"/>
            <rectangle  name="ta" x="0"   y="90" width="600"  height="12" draw="false" fill="false" color="#0000FF"/>
            <label      name="zo" x="0"   y="90" width="600"  height="12" fontsize="10" dynamic="false" alignment="center" vertical-aligment="middle" fsbold="true">INVENTARIO GENERAL VALORADO</label>
	    <label      name="zo" x="225"   y="110" width="50"  height="12" fontsize="10" dynamic="false" alignment="right" vertical-aligment="middle" fsbold="true">AÑO: </label>
	    <string-field         x="275"   y="110"  width="50" height="12" fontsize="10" dynamic="true"  alignment="left" fsbold="true" fieldname="anio"/>
	    
	    <rectangle name="tangle"  x="0"     y="150" width="450"  height="16"  draw="true" fill="false" color="#000000"/>
	    <rectangle name="tangle"  x="450"   y="150" width="60"  height="16"  draw="true" fill="false" color="#000000"/>
	    <rectangle name="tangle"  x="510"   y="150" width="50"  height="16"  draw="true" fill="false" color="#000000"/>
	    
	    <label name="resource"          x="0"   y="150" width="450" height="16" fontsize="08" dynamic="true" alignment="center" vertical-alignment="middle">Recurso</label>
	    <label name="exixtencia"        x="450" y="150" width="60" height="16" fontsize="08" dynamic="true" alignment="center" vertical-alignment="middle">Existencia</label>
	    <label name="subtotal"          x="510" y="150" width="50"  height="16" fontsize="08" dynamic="true" alignment="center" vertical-alignment="middle">Subtotal</label>
	    
	</pageheader>
	
        <items layout-cachable="false" >
            <band name="table_0" fontsize="08">
		    
		    <line x1="0"   y1="0" x2="560"   y2="0" weight="0.25"/>
                    <line x1="0"   y1="0" x2="0"   y2="100%" weight="0.25"/>
                    <line x1="450" y1="0" x2="450" y2="100%" weight="0.25"/>
                    <line x1="510" y1="0" x2="510" y2="100%" weight="0.25"/>
                    <line x1="560" y1="0" x2="560" y2="100%" weight="0.25"/>

                    <string-field name="Recurso"      x="3"    y="0"  width="447" height="10" alignment="left" vertical-alignment="middle" dynamic="false" fieldname="table_0.recurso"/>
                    <number-field name="Existencia"   x="450"  y="0"  width="50"  height="10" alignment="right" vertical-alignment="middle" dynamic="false" fieldname="table_0.cantidad"/>
                    <number-field name="subTotal"     x="510"  y="0"  width="45"  height="10" alignment="right" vertical-alignment="middle" dynamic="false" format="$ #,##0.00"  fieldname="table_0.totalprice"/>
            </band>
	    
	</items>
	
	<pagefooter>
	    <line                                x1="0"    y1="0"   x2="560"     y2="0"    weight="0.5"/>
	    <string-field fieldname="pageXofY"   x="0"     y="10"    width="560"  height="12" alignment="right" vertical-alignment="top"/> 
	</pagefooter>

	<reportfooter height="20" fontname="Monospaced" fontstyle="bold" fontsize="9">
		<line                                x1="0"    y1="2"    x2="5100"     y2="2"     weight="0.5"/>
		<label name="PatrimonioNeto"         x="350"   y="20"    width="100"  height="12" fontsize="08" dynamic="true" alignment="center" fsbold="true" vertical-alignment="middle">Patrimonio Neto:</label>
		<rectangle                           x="450" y="20" width="110" height="15" color="#FFFF00" draw="true" fill="true"/>
		<rectangle                           x="450" y="20" width="110" height="15" color="#000000" draw="true" fill="false"/>
		<number-field              	     x="450"   y="20" format="$ #,##0.00" fieldname="patrimonioNetoTotal" dynamic="true" width="100" alignment="right" fontname="SansSerif" fsbold="true" fontsize="10"/>
		<line                      	     x1="450"  y1="340"   x2="550"     y2="340"     weight="0.5"/>
		<label name="Patrimonio"   	     x="450"   y="360"    width="100"  height="12" fontsize="08" dynamic="true" alignment="center" vertical-alignment="middle">Patrimonio</label>
	</reportfooter>

	<groups>
		<groupfooter height="10" fontname="Monospaced" fontstyle="bold" fontsize="9">
		    <line x1="0" y1="0" x2="550" y2="0" weight="0.5" color="#000000"/>
		</groupfooter>
	</groups>

	<functions>

		<function class="org.jfree.report.function.HideElementByNameFunction" name="hideHeader">
			<properties>
				<property name="element">table_0</property>
				<property name="field">TablePrefix</property>
			</properties>
		</function>
		
		<function class="org.jfree.report.function.HideElementByNameFunction" name="hideDetail">
			<properties>
				<property name="element">table_0</property>
				<property name="field">TablePrefix</property>
			</properties>
		</function>

		<function class="org.jfree.report.function.PageOfPagesFunction" name="pageXofY">
		<properties>
			<property name="format">Página {0} de {1}</property>
		</properties>
		
		
	    </function>
	    
	    <function class="org.jfree.report.function.ItemSumFunction" name="patrimonioNetoTotal">
			<properties>
				<property name="field">table_0.totalprice</property>
				<property name="group">Header-Detail</property>
			</properties>
	    </function>
	    
	</functions>
        
        <pagefooter height="24">
                <rectangle name="PF0" x="0" y="0" width="100%" height="100%" color="#FFFFFF" weight="0" draw="false" fill="true"/>
                <line x1="0" y1="0" x2="100%" y2="0" weight="0.5"/>
                <string-field fieldname="location" x="0" y="4" width="100%" height="100%" alignment="left" vertical-alignment="middle"/>
                <string-field fieldname="pageXofY" x="0" y="4" width="100%" height="100%" alignment="right" vertical-alignment="middle"/>
        </pagefooter>
</report>




