            <string-field             x="270" y="115" width="065" dynamic="true" alignment="left"  fontsize="08" fieldname="enddate" fsbold="false"/>    
	</pageheader>
        
        	
	<groups>
		<group name="table_0">
			<fields>
				<field>TablePrefix</field>
			</fields>
			
			<groupheader repeat="true" layout-cachable="false" pagebreak-before-print="false">
                            <band name="table_0" height="15" fontname="SansSerif" fontsize="09" >
		
                                        <rectangle name="tangle"  x="0"   y="12" width="60"  height="12"  draw="true" fill="true" color="#E5E5E5"/>
                                        <rectangle name="tangle"  x="60"  y="12" width="60"  height="12"  draw="true" fill="true" color="#E5E5E5"/>
                                        <rectangle name="tangle"  x="120" y="12" width="170" height="12"  draw="true" fill="true" color="#E5E5E5"/>
                                        <rectangle name="tangle"  x="290" y="12" width="60" height="12"  draw="true" fill="true" color="#E5E5E5"/>
                                        <rectangle name="tangle"  x="350" y="12" width="160" height="12"  draw="true" fill="true" color="#E5E5E5"/>
                                        
                                        <rectangle name="tangle"  x="0"   y="12" width="60"  height="12"  draw="true" fill="false" color="#000000"/>
                                        <rectangle name="tangle"  x="60"  y="12" width="60"  height="12"  draw="true" fill="false" color="#000000"/>
                                        <rectangle name="tangle"  x="120" y="12" width="170" height="12"  draw="true" fill="false" color="#000000"/>
                                        <rectangle name="tangle"  x="290" y="12" width="60" height="12"  draw="true" fill="false" color="#000000"/>
                                        <rectangle name="tangle"  x="350" y="12" width="160" height="12"  draw="true" fill="false" color="#000000"/>
                                        
					<label name="resou" x="0"   y="13" width="60"  height="11" dynamic="true" alignment="center" fsbold="true">Nº Orden</label>
					<label name="brand" x="60"  y="13" width="60"  height="11" dynamic="true" alignment="center" fsbold="true">Fecha</label>
					<label name="quati" x="120" y="13" width="170" height="11" dynamic="true" alignment="center" fsbold="true">Proveedor</label>
					<label name="unit"  x="290" y="13" width="60"  height="11" dynamic="true" alignment="center" fsbold="true">($) Costo</label>
					<label name="unit"  x="350" y="13" width="160" height="11" dynamic="true" alignment="center" fsbold="true">Motivo</label>
                                        
				</band>
			</groupheader>
			
			<groupfooter height="18" fontname="Monospaced" fontstyle="bold" fontsize="9">
			    <line x1="0" y1="0" x2="510" y2="0" weight="0.25"/>
      			</groupfooter>
		</group>
		
		
	</groups>
	
	<items layout-cachable="false" >
		<band name="table_0" fontsize="08">
			<line x1="0"   y1="0" x2="0"   y2="100%" weight="0.25"/>
			<line x1="60"  y1="0" x2="60"  y2="100%" weight="0.25"/>
			<line x1="120" y1="0" x2="120" y2="100%" weight="0.25"/>
                        <line x1="290" y1="0" x2="290" y2="100%" weight="0.25"/>
                        <line x1="350" y1="0" x2="350" y2="100%" weight="0.25"/>
                        <line x1="510" y1="0" x2="510" y2="100%" weight="0.25"/>
			
			<number-field x="5"   y="2" width="55"  alignment="center"  vertical-alignment="middle" height="10" dynamic="false" format="000000" fieldname="table_0.number"/>
			<date-field   x="62"  y="2" width="60"  alignment="center"  vertical-alignment="middle" height="10" dynamic="false" fieldname="table_0.date" format="dd/MM/yyyy"/>
			<string-field x="125" y="2" width="165" alignment="left"     vertical-alignment="middle" height="10" dynamic="false" fieldname="table_0.proveedor"/>
			<number-field x="295" y="2" width="54"  alignment="right"   vertical-alignment="middle" height="10" dynamic="false" fieldname="table_0.amount" format="$ #,##0.00"/>
                        <string-field x="355" y="2" width="158" alignment="left"    vertical-alignment="middle" height="10" dynamic="false" fieldname="table_0.description"/>
		</band>
	</items>
	
        <pagefooter height="24">
                <rectangle name="PF0" x="0" y="0" width="100%" height="100%" color="#FFFFFF" weight="0" draw="false" fill="true"/>
                <line x1="0" y1="0" x2="100%" y2="0" weight="0.5"/>
                <string-field fieldname="location" x="0" y="4" width="100%" height="100%" alignment="left" vertical-alignment="middle"/>
                <string-field fieldname="pageXofY" x="0" y="4" width="100%" height="100%" alignment="right" vertical-alignment="middle"/>
        </pagefooter>

    <functions>
	<property-ref name="report.date"/>
	    <function class="org.jfree.report.function.PageOfPagesFunction" name="pageXofY">
		<properties>
			<property name="format">Página {0} de {1}</property>
		</properties>
	    </function>
    
    </functions>
	
	<functions>
		<function class="org.jfree.report.function.HideElementByNameFunction" name="hideHeader">
			<properties>
				<property name="element">table_0</property>
				<property name="field">TablePrefix</property>
			</properties>
		</function>
	</functions>
	
</report>


