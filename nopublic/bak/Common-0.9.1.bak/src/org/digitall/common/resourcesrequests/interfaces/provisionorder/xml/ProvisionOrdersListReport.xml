            <string-field          x="450" y="65"  width="200" height="10" alignment="left" fontsize="08" fsbold="false" dynamic="true" format="dd/MM/yyyy" fieldname="actualdate"/>
            
            <label  x="0"   y="85"  width="100%" dynamic="true" fontsize="09" fsbold="true" alignment="center">ÓRDENES DE PROVISIÓN REALIZADAS</label>
            <label name="lbl"         x="090" y="100" width="100" dynamic="true" alignment="right" fontsize="09" fsbold="true">Centro de Costos: </label>
