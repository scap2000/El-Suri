					<label name="label01"  x="0"   y="50" width="85"  height="18" dynamic="false" alignment="right" vertical-alignment="middle">Nº Expediente:</label>
                                        <string-field x="95"   y="50" width="425"  height="18" dynamic="false" alignment="left" vertical-alignment="middle" fieldname="table_0.expte"/>
