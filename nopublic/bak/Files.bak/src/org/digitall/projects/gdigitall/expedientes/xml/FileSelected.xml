                                        <rectangle name="tangle"  x="90" y="86" width="425"  height="18"  draw="true" fill="false" color="#000000"/>
                                        
                                        <label name="label04"  x="0" y="104" width="85" height="18" dynamic="false" alignment="right" vertical-alignment="middle">Iniciador:</label>
