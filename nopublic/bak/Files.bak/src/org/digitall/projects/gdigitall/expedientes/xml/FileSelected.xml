                                        <label name="label06"  x="0" y="140" width="85"  height="18" dynamic="false" alignment="right" vertical-alignment="middle">Iniciante:</label>
                                        <string-field x="95"   y="140" width="425"  height="18" dynamic="false" alignment="left" vertical-alignment="middle" fieldname="table_0.iniciante"/>
                                        <rectangle name="tangle"  x="90" y="140" width="425"  height="18"  draw="true" fill="false" color="#000000"/>
                                        
                                        <label name="label07"  x="0" y="158" width="85" height="18" alignment="right" vertical-alignment="middle">Contenido:</label>
                                        <string-field x="95"   y="158" width="425"  height="36" dynamic="false" alignment="left" vertical-alignment="middle" fieldname="table_0.extracto"/>
                                        <rectangle name="tangle"  x="90" y="158" width="425"  height="36"  draw="true" fill="false" color="#000000"/>
                                        
                                        <label name="label08"  x="0" y="194" width="85"  height="18" alignment="right" vertical-alignment="middle">Observaciones:</label>
                                        <string-field x="95"   y="194" width="425"  height="36" dynamic="false" alignment="left" vertical-alignment="middle" fieldname="table_0.observaciones"/>
                                        <rectangle name="tangle"  x="90" y="194" width="425"  height="36"  draw="true" fill="false" color="#000000"/>
                                        
                                        <label name="label09"    x="0" y="230" width="85"  height="18" dynamic="false" alignment="right" vertical-alignment="middle">Folios:</label>
                                        <string-field x="95"   y="230" width="425"  height="18" dynamic="false" alignment="left" vertical-alignment="middle" fieldname="table_0.cantfolios"/>
                                        <rectangle name="tangle"  x="90" y="230" width="425"  height="18"  draw="true" fill="false" color="#000000"/>
                                        
                                        <label name="label10" x="0" y="248" width="85" height="18" alignment="right" vertical-alignment="middle">Estado:</label>
                                        <string-field x="95"   y="248" width="425"  height="18" dynamic="false" alignment="left" vertical-alignment="middle" fieldname="table_0.estadoexp"/>
                                        <rectangle name="tangle"  x="90" y="248" width="425"  height="18"  draw="true" fill="false" color="#000000"/>
                                        
                                        <label name="label11" x="0" y="266" width="85"  height="18" alignment="right" vertical-alignment="middle">Profesional:</label>
                                        <string-field x="95"   y="266" width="425"  height="18" dynamic="false" alignment="left" vertical-alignment="middle" fieldname="table_0.profesional"/>
                                        <rectangle name="tangle"  x="90" y="266" width="425"  height="18"  draw="true" fill="false" color="#000000"/>
                                        
