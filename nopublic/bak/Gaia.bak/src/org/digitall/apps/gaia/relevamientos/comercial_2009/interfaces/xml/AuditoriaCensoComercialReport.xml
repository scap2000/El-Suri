<?xml version="1.0" encoding="ISO-8859-1" ?>

<report xmlns="http://jfreereport.sourceforge.net/namespaces/reports/legacy/simple" name="Auditoría Censo Comercial 2009" pageformat="A4"
       topmargin="25" leftmargin="25" bottommargin="10" rightmargin="10">
	
        <pageheader height="12" fontname="SansSerif" fontsize="08" fsbold="false">
	    
