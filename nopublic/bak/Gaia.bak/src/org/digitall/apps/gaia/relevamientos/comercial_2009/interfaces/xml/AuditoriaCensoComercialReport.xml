	    <line x1="370" y1="0" x2="370" y2="80"  weight=".75" color="#AAAAAA"/>
	    
	    <image-field name="logo" x="0"   y="5" width="52"  height="30"  scale="true" fieldname="logo"/>
	    <image-field name="logo" x="315" y="5" width="52"  height="30"  scale="true" fieldname="logo"/>
	    
	    <rectangle name="ta" x="65"  y="0"  width="250" height="28" draw="false" fill="false" color="#0000FF"/>
	    <string-field        x="65"  y="00" width="250" height="28" dynamic="false" alignment="center" vertical-aligment="middle" fontsize="14" fsbold="true" fieldname="orgname"/>
	    <rectangle name="ta" x="65"  y="30" width="250" height="10" draw="false" fill="false" color="#0000FF"/>
	    <string-field        x="65"  y="30" width="250" height="10" dynamic="false" alignment="center" fontsize="08" fsbold="false" vertical-aligment="middle" fieldname="locationdata"/>
	    <rectangle name="ta" x="0"   y="52" width="230" height="10" draw="false" fill="false" color="#0000FF"/>
