	    <rectangle name="ta" x="0"   y="65" width="190" height="10" draw="false" fill="false" color="#0000FF"/>
	    <string-field        x="0"   y="65" width="190" height="10" dynamic="false" alignment="left" fontsize="08" fieldname="webaddress"/>
	    
	    <label     name="zo" x="380" y="00"  width="200" height="10" fontsize="08" dynamic="false" alignment="center" vertical-aligment="middle" fsbold="true">CENSO COMERCIAL 2009</label>
	    <label     name="zo" x="380" y="20"  width="200" height="10" fontsize="08" dynamic="false" alignment="center" vertical-aligment="middle" fsbold="true">AUDITORÍA</label>
	    
	    <rectangle name="ta" x="380" y="55"  width="60"  height="10" draw="false" fill="false" color="#0000FF"/>
	    <label     name="zo" x="380" y="65"  width="60"  height="10" fontsize="08" dynamic="false" alignment="left" vertical-aligment="middle" fsbold="false">Fecha Impr.:</label>
	    <rectangle name="ta" x="470" y="65"  width="200" height="10" draw="false" fill="false" color="#0000FF"/>
	    <string-field        x="470" y="65"  width="200" height="10" fontsize="08" dynamic="true"  alignment="left" fieldname="actualdate"/>
	
	    <rectangle name="t01" x="0"   y="90" width="120" height="12"  draw="true" fill="false" color="#000000"/>
	    <rectangle name="t02" x="120" y="90" width="50"  height="12"  draw="true" fill="false" color="#000000"/>
	    <rectangle name="t03" x="170" y="90" width="80"  height="12"  draw="true" fill="false" color="#000000"/>
	    <rectangle name="t04" x="250" y="90" width="100" height="12"  draw="true" fill="true" color="#DCDCDC"/>
	    <rectangle name="t04" x="250" y="90" width="100" height="12"  draw="true" fill="false" color="#000000"/>
	    <rectangle name="t05" x="350" y="90" width="60"  height="12"  draw="true" fill="false" color="#000000"/>
	    <rectangle name="t06" x="410" y="90" width="140" height="12"  draw="true" fill="false" color="#000000"/>

	    
	    <label name="coord"  x="0"   y="90" width="120" height="12" fontsize="08" dynamic="true" alignment="center" vertical-alignment="middle">Coordenada</label>
	    <label name="cat"    x="120" y="90" width="50"  height="12" fontsize="08" dynamic="true" alignment="center" vertical-alignment="middle">Catastro</label>
	    <label name="codenc" x="170" y="90" width="80"  height="12" fontsize="08" dynamic="true" alignment="center" vertical-alignment="middle">Nº  P - Z - E</label>
	    <label name="fecha"  x="250" y="90" width="100" height="12" fontsize="08" dynamic="true" alignment="center" vertical-alignment="middle">Fecha de Carga</label>
	    <label name="user"   x="350" y="90" width="60"  height="12" fontsize="08" dynamic="true" alignment="center" vertical-alignment="middle">Usuario</label>
	    <label name="razs"   x="410" y="90" width="140" height="12" fontsize="08" dynamic="true" alignment="center" vertical-alignment="middle">Razón Social</label>
	</pageheader>
	
         <groups>
		<group name="Header-Footer">
			<groupfooter height="18" fontname="Monospaced" fontstyle="bold" fontsize="9">
                                <line x1="0" y1="0" x2="550" y2="0" weight="0.25"/>
      			</groupfooter>
		</group>
	</groups>
		
        <items layout-cachable="false" >
            <band name="table_0" fontsize="08">
                    
                    <line x1="0"   y1="0" x2="0"   y2="100%" weight="0.25"/>
                    <line x1="120" y1="0" x2="120" y2="100%" weight="0.25"/>
                    <line x1="170" y1="0" x2="170" y2="100%" weight="0.25"/>
                    <line x1="250" y1="0" x2="250" y2="100%" weight="0.25"/>
                    <line x1="350" y1="0" x2="350" y2="100%" weight="0.25"/>
		    <line x1="410" y1="0" x2="410" y2="100%" weight="0.25"/>
                    <line x1="550" y1="0" x2="550" y2="100%" weight="0.25"/>

                    <string-field name="fcood"   x="0"   y="2"  width="120" height="10" alignment="center" vertical-alignment="middle" dynamic="false" fieldname="table_0.coordenada"/>
                    <string-field name="fcat"    x="120" y="2"  width="50"  height="10" alignment="center" vertical-alignment="middle" dynamic="false" fieldname="table_0.f1k"/>
                    <string-field name="fcodenc" x="173" y="2"  width="77"  height="10" alignment="left"   vertical-alignment="middle" dynamic="false" fieldname="table_0.codencuesta"/>
                    <date-field   name="ffecha"  x="250" y="2"  width="97"  height="10" alignment="center" vertical-alignment="middle" dynamic="false" format="dd/MM/yyyy HH:mm:ss" fieldname="table_0.fechacarga"/>
                    <string-field name="fuser"   x="353" y="2"  width="57"  height="10" alignment="left"   vertical-alignment="middle" dynamic="false" fieldname="table_0.usuario"/>
                    <string-field name="frazs"   x="413" y="2"  width="137" height="10" alignment="left"   vertical-alignment="middle" dynamic="false" fieldname="table_0.f2b"/>
            </band>
	</items>

	<pagefooter height="20" fontname="Monospaced" fontstyle="bold" fontsize="9">
		<string-field fieldname="pageXofY" x="0" y="2" width="550" height="12" alignment="right" vertical-alignment="top"/> 
		<line x1="0" y1="0" x2="550" y2="0" weight="0.5"/>
	</pagefooter>

	<functions>

		<function class="org.jfree.report.function.HideElementByNameFunction" name="hideHeader">
			<properties>
				<property name="element">table_0</property>
				<property name="field">TablePrefix</property>
			</properties>
		</function>
		
		<function class="org.jfree.report.function.HideElementByNameFunction" name="hideDetail">
			<properties>
				<property name="element">table_0</property>
				<property name="field">TablePrefix</property>
			</properties>
		</function>

                <function class="org.jfree.report.function.ItemSumFunction" name="deuda">
			<properties>
				<property name="field">table_0.deuda</property>
				<property name="group">Header-Detail</property>
			</properties>
		</function>
                
                <property-ref name="dummy-field"/>
		
		<function class="org.jfree.report.function.PageOfPagesFunction" name="pageXofY">
		<properties>
			<property name="format">Página {0} de {1}</property>
		</properties>
	    </function>

	</functions>
</report>



