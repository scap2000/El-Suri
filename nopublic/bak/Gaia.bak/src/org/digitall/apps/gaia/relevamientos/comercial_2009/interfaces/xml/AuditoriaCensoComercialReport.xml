	    <rectangle name="ta" x="230" y="52" width="45"  height="10" draw="false" fill="false" color="#0000FF"/>
	    <label     name="ti" x="230" y="52" width="45"  height="10" dynamic="false" alignment="left" fontsize="08" fsbold="false">Teléfonos:</label>
	    <rectangle name="ta" x="275" y="52" width="90"  height="10" draw="false" fill="false" color="#0000FF"/>
	    <string-field        x="275" y="52" width="90"  height="10" dynamic="false" alignment="left" fontsize="08" fieldname="phonenumber1"/>
	    <rectangle name="ta" x="275" y="65" width="90"  height="10" draw="false" fill="false" color="#0000FF"/>
