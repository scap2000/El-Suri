	</pageheader>
	
	<groups>
		<group name="Header-Detail" >
			<fields>
				<field>TablePrefix</field>
				<field>table_0.header</field>
			</fields>
			
			<groupheader repeat="false" layout-cachable="false" >
			</groupheader>
			
			<groupfooter height="10" fontname="Monospaced" fontstyle="bold" fontsize="9">
			<line x1="0" y1="0" x2="550" y2="0" weight="0.5" color="#000000"/>
      			</groupfooter>
		</group>
		
	</groups>
	
	<items layout-cachable="false" >
            <band name="table_0" fontsize="08">
                        
		    <line x1="0"   y1="0" x2="0"   y2="100%" weight="0.25"/>
                    <line x1="290" y1="0" x2="290" y2="100%" weight="0.25"/>
                    <line x1="390" y1="0" x2="390" y2="100%" weight="0.25"/>
                    <line x1="470" y1="0" x2="470" y2="100%" weight="0.25"/>
                    <line x1="550" y1="0" x2="550" y2="100%" weight="0.25"/>

                    <string-field name="Recurso"             x="3"    y="2"  width="287" height="08" alignment="left"   vertical-alignment="middle" dynamic="false" fieldname="table_0.resourcename"/>
                    <string-field name="Marca"               x="293"  y="2"  width="97"  height="08" alignment="left"   vertical-alignment="middle" dynamic="false" fieldname="table_0.brandname"/>
                    <string-field name="Nro.Patrimonio"      x="390"  y="2"  width="80"  height="08" alignment="center" vertical-alignment="middle" dynamic="false"  fieldname="table_0.numpatrimonio"/>
                    <date-field   name="Fecha de Asignacion" x="470"  y="2"  width="80"  height="08" alignment="center" vertical-alignment="middle" dynamic="false" format="dd/MM/yyyy" fieldname="table_0.dateassigned"/>
		    
		</band>
	</items>
        
    <pagefooter height="20" fontsize="9">
		<line                              x1="0"    y1="40"   x2="100"     y2="40"     weight="0.5"/>
		<label name="Patrimonio"           x="0"     y="50"    width="100"  height="12" fontsize="08" dynamic="true" alignment="center" vertical-alignment="middle">Patrimonio</label>
		<line                              x1="450"  y1="40"   x2="550"     y2="40"     weight="0.5"/>
		<label name="Empleado"             x="450"   y="50"    width="100"  height="12" fontsize="08" dynamic="true" alignment="center" vertical-alignment="middle">Empleado</label>
		<line                              x1="0"    y1="70"   x2="550"     y2="70"    weight="0.5"/>
                <string-field fieldname="location" x="0"     y="75"     width="100%" height="12" alignment="left" vertical-alignment="middle"/>
		<string-field fieldname="pageXofY" x="0"     y="75"    width="550"  height="12" alignment="right" vertical-alignment="top"/> 
    </pagefooter>
	

    <functions>
	<property-ref name="report.date"/>
	    <function class="org.jfree.report.function.PageOfPagesFunction" name="pageXofY">
		<properties>
			<property name="format">Página {0} de {1}</property>
		</properties>
	    </function>
    
    </functions>
    
</report>